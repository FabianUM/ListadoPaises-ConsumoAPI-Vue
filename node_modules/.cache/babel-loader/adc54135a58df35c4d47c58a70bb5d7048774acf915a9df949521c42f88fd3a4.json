{"ast":null,"code":"import gql from 'graphql-tag';\nimport { apolloClient } from '@/apollo-client';\nimport pixabayClient from '@/axios';\nexport default {\n  name: 'HomeView',\n  data() {\n    return {\n      countries: []\n    };\n  },\n  async mounted() {\n    try {\n      const response = await apolloClient.query({\n        query: gql`\n          query {\n            countries {\n              code\n              name\n              continent {\n                name\n              }\n              capital\n            }\n          }\n        `\n      });\n      const countries = response.data.countries;\n\n      // Fetch flag image and capital image for each country\n      for (let country of countries) {\n        // Fetch flag image\n        const flagResponse = await pixabayClient.get('', {\n          params: {\n            q: country.name + ' flag',\n            image_type: 'photo',\n            per_page: 1\n          }\n        });\n        const flagImage = flagResponse.data.hits[0]?.webformatURL;\n        country.flagImage = flagImage;\n\n        // Fetch capital image\n        const capitalResponse = await pixabayClient.get('', {\n          params: {\n            q: country.capital,\n            image_type: 'photo',\n            per_page: 1\n          }\n        });\n        const capitalImage = capitalResponse.data.hits[0]?.webformatURL;\n        country.capitalImage = capitalImage;\n      }\n      this.countries = countries;\n    } catch (error) {\n      console.error('Error fetching countries or images:', error);\n    }\n  }\n};","map":{"version":3,"names":["gql","apolloClient","pixabayClient","name","data","countries","mounted","response","query","country","flagResponse","get","params","q","image_type","per_page","flagImage","hits","webformatURL","capitalResponse","capital","capitalImage","error","console"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<!--<template>\r\n  <div>\r\n    <div class=\"card-deck\">\r\n      <div class=\"card\" v-for=\"country in countries\" :key=\"country.code\">\r\n        <img class=\"card-img-top\" :src=\"country.image\" alt=\"Country image\">\r\n        <div class=\"card-body\">\r\n          <h5 class=\"card-title\">{{ country.name }}</h5>\r\n          <p class=\"card-text\">{{ country.continent.name }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport gql from 'graphql-tag';\r\nimport { apolloClient } from '@/apollo-client';\r\nimport pixabayClient from '@/axios';\r\n\r\nexport default {\r\n  name: 'HomeView',\r\n  data() {\r\n    return {\r\n      countries: [],\r\n    };\r\n  },\r\n  async mounted() {\r\n    try {\r\n      const response = await apolloClient.query({\r\n        query: gql`\r\n          query {\r\n            countries {\r\n              code\r\n              name\r\n              continent {\r\n                name\r\n              }\r\n            }\r\n          }\r\n        `,\r\n      });\r\n\r\n      let countries = response.data.countries;\r\n\r\n      // Limitar a las primeras 12 países\r\n      countries = countries.slice(0, 12);\r\n\r\n      // Fetch images from Pixabay for each country\r\n      for (let country of countries) {\r\n        const imageResponse = await pixabayClient.get('', {\r\n          params: {\r\n            q: country.name,\r\n            image_type: 'photo',\r\n            per_page: 3,  // Ajusta según sea necesario\r\n          },\r\n        });\r\n        const image = imageResponse.data.hits[0]?.webformatURL;\r\n        country.image = image;\r\n      }\r\n\r\n      this.countries = countries;\r\n    } catch (error) {\r\n      console.error('Error fetching countries or images:', error);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.card-deck {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: space-around;\r\n}\r\n.card {\r\n  width: 18rem;\r\n  margin-bottom: 1rem;\r\n}\r\n</style>-->\r\n\r\n<template>\r\n  <div>\r\n    <div class=\"card-deck\">\r\n      <div class=\"card\" v-for=\"country in countries\" :key=\"country.code\">\r\n        <img class=\"card-img-top\" :src=\"country.flagImage\" alt=\"Country flag\">\r\n        <div class=\"card-body\">\r\n          <div class=\"country-info\">\r\n            <div class=\"flag-title\">\r\n              <img class=\"flag-icon\" :src=\"country.flagImage\" alt=\"Country flag\">\r\n              <h5 class=\"card-title\">{{ country.name }}</h5>\r\n            </div>\r\n            <div class=\"capital\">\r\n              <img class=\"capital-img\" :src=\"country.capitalImage\" alt=\"Capital image\">\r\n              <p class=\"capital-name\">{{ country.capital }}</p>\r\n            </div>\r\n            <p class=\"card-text continent\">{{ country.continent.name }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport gql from 'graphql-tag';\r\nimport { apolloClient } from '@/apollo-client';\r\nimport pixabayClient from '@/axios';\r\n\r\nexport default {\r\n  name: 'HomeView',\r\n  data() {\r\n    return {\r\n      countries: [],\r\n    };\r\n  },\r\n  async mounted() {\r\n    try {\r\n      const response = await apolloClient.query({\r\n        query: gql`\r\n          query {\r\n            countries {\r\n              code\r\n              name\r\n              continent {\r\n                name\r\n              }\r\n              capital\r\n            }\r\n          }\r\n        `,\r\n      });\r\n\r\n      const countries = response.data.countries;\r\n\r\n      // Fetch flag image and capital image for each country\r\n      for (let country of countries) {\r\n        // Fetch flag image\r\n        const flagResponse = await pixabayClient.get('', {\r\n          params: {\r\n            q: country.name + ' flag',\r\n            image_type: 'photo',\r\n            per_page: 1,\r\n          },\r\n        });\r\n        const flagImage = flagResponse.data.hits[0]?.webformatURL;\r\n        country.flagImage = flagImage;\r\n\r\n        // Fetch capital image\r\n        const capitalResponse = await pixabayClient.get('', {\r\n          params: {\r\n            q: country.capital,\r\n            image_type: 'photo',\r\n            per_page: 1,\r\n          },\r\n        });\r\n        const capitalImage = capitalResponse.data.hits[0]?.webformatURL;\r\n        country.capitalImage = capitalImage;\r\n      }\r\n\r\n      this.countries = countries;\r\n    } catch (error) {\r\n      console.error('Error fetching countries or images:', error);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.card-deck {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: space-around;\r\n}\r\n.card {\r\n  width: 18rem;\r\n  margin-bottom: 1rem;\r\n}\r\n.flag-title {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.flag-icon {\r\n  width: 30px; /* Ajusta el tamaño según sea necesario */\r\n  height: auto;\r\n  margin-right: 10px;\r\n}\r\n.capital {\r\n  margin-top: 10px;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.capital-img {\r\n  width: 50px; /* Ajusta el tamaño según sea necesario */\r\n  height: auto;\r\n  margin-right: 10px;\r\n}\r\n.capital-name {\r\n  margin: 0;\r\n}\r\n.continent {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n\r\n\r\n"],"mappings":"AAwGA,OAAAA,GAAA;AACA,SAAAC,YAAA;AACA,OAAAC,aAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;IACA;EACA;EACA,MAAAC,QAAA;IACA;MACA,MAAAC,QAAA,SAAAN,YAAA,CAAAO,KAAA;QACAA,KAAA,EAAAR,GAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;MACA;MAEA,MAAAK,SAAA,GAAAE,QAAA,CAAAH,IAAA,CAAAC,SAAA;;MAEA;MACA,SAAAI,OAAA,IAAAJ,SAAA;QACA;QACA,MAAAK,YAAA,SAAAR,aAAA,CAAAS,GAAA;UACAC,MAAA;YACAC,CAAA,EAAAJ,OAAA,CAAAN,IAAA;YACAW,UAAA;YACAC,QAAA;UACA;QACA;QACA,MAAAC,SAAA,GAAAN,YAAA,CAAAN,IAAA,CAAAa,IAAA,KAAAC,YAAA;QACAT,OAAA,CAAAO,SAAA,GAAAA,SAAA;;QAEA;QACA,MAAAG,eAAA,SAAAjB,aAAA,CAAAS,GAAA;UACAC,MAAA;YACAC,CAAA,EAAAJ,OAAA,CAAAW,OAAA;YACAN,UAAA;YACAC,QAAA;UACA;QACA;QACA,MAAAM,YAAA,GAAAF,eAAA,CAAAf,IAAA,CAAAa